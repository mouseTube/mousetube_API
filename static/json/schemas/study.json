{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Study",
	"type": "object",
	"properties": {
	  "name": {
		"type": "string",
		"title": "Study Name",
		"description": "Name of the study."
	  },
	  "description": {
		"type": "string",
		"title": "Description",
		"description": "Description of the study."
	  },
	  "start_date": {
		"type": "string",
		"format": "date-time",
		"title": "Start Date",
		"description": "Start date of the study (YYYY-MM-DD hh:mm:ss)."
	  },
	  "end_date": {
		"type": "string",
		"format": "date-time",
		"title": "End Date",
		"description": "End date of the study (YYYY-MM-DD hh:mm:ss)."
	  }
	},
	"required": ["name", "start_date"]
  }